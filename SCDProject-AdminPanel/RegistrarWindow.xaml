<Window x:Class="SCDProject_AdminPanel.RegistrarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCDProject_AdminPanel"
        mc:Ignorable="d"
        Title="RegistrarWindow" Height="600" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*" />
            <RowDefinition Height="*" />

        </Grid.RowDefinitions>
        <DataGrid Margin="150,30" x:Name="requestGrid" IsReadOnly="True" />
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Click="ApproveButton_Click" Width="150" Height="40" Margin="40,0,40,10" Background="Green" Foreground="White">Approve</Button>
            <Button Click="RejectButton_Click" Width="150" Height="40" Margin="40,0,40,10" Background="Red" Foreground="White">Reject</Button>
        </StackPanel>
    </Grid>
</Window>
